# Usando for para imprimir los números del 1 al 5
print("Números con for:")
for i in range(1, 6):
    print(i, end=" ")
print()  # Salto de línea

# Usando while para imprimir los números del 1 al 5
print("Números con while:")
num = 1
while num <= 5:
    print(num, end=" ")
    num += 1
print()  # Salto de línea
